#include <stdio.h>
main()
{
    int nc;
    long c;
    c=0;
    while((nc=getchar()) != EOF)
    {
        if(nc==' '||nc=='\t')
        {
            putchar(' ');
            while((nc=getchar())==' '||nc=='\t')
                ;
            putchar(nc);
        }
        else
            putchar(nc);
    }
}
